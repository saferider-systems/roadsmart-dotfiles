# Configuration for Roadsmart Dotfiles repository
Host roadsmart-croncomm
	HostName github.com
	User git
	IdentityFile ~/.ssh/roadsmart-croncomm-service
	IdentitiesOnly yes
  StrictHostKeyChecking accept-new

Host roadsmart-dotfiles
	HostName github.com
	User git
	IdentityFile ~/.ssh/roadsmart-dotfiles
	IdentitiesOnly yes
  StrictHostKeyChecking accept-new

Host roadsmart-docker
	HostName github.com
	User git
	IdentityFile ~/.ssh/roadsmart-docker
	IdentitiesOnly yes
	StrictHostKeyChecking accept-new

Host roadsmart-gateway-5.0
	HostName github.com
	User git
	IdentityFile ~/.ssh/roadsmart-gateway-5.0
	IdentitiesOnly yes
  StrictHostKeyChecking accept-new

