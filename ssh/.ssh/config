	IdentityFile ~/.ssh/roadsmart-gateway-5.0.1
	IdentitiesOnly yes
  StrictHostKeyChecking accept-new

Host roadsmart-socket
	HostName github.com
	User git
	IdentityFile ~/.ssh/roadsmart-socket-service
  IdentitiesOnly yes
  StrictHostKeyChecking accept-new

Host roadsmart-web
	HostName github.com
	User git
	IdentityFile ~/.ssh/roadsmart-web-platform
	IdentitiesOnly yes
	StrictHostKeyChecking accept-new
